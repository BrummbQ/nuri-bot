{
  "manifest_version": 3,
  "name": "Nuri Bot Basket Helper",
  "description": "Integrate with rewe shopping basket",
  "version": "1.0.1",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["cookies", "scripting", "activeTab", "tabs"],
  "host_permissions": [
    "*://*.rewe.de/",
    "http://localhost:3000/",
    "https://nuribot.de/"
  ]
}
